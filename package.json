{
  "name": "rozgar.today",
  "version": "0.0.2-beta.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yashanand1910/rozgar.today.git"
  },
  "bugs": {
    "url": "https://github.com/yashanand1910/rozgar.today/issues"
  },
  "engines": {
    "node": "14"
  },
  "homepage": "https://github.com/yashanand1910/rozgar.today#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clone:data": "bash clone-data.bash",
    "emulate:backend": "firebase emulators:start --import ./data-clone",
    "install:backend": "cd functions && npm install && cd ..",
    "build:alpha": "ng build",
    "build:beta": "ng build --configuration=beta",
    "build": "ng build --prod",
    "deploy:alpha": "firebase deploy --only hosting:alpha",
    "deploy:beta": "firebase deploy --only hosting:beta",
    "deploy": "firebase deploy --only hosting:stable",
    "deploy:backend": "firebase deploy --except hosting",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:remoteconfig": "firebase deploy --only remoteconfig",
    "release": "standard-version",
    "release:beta": "npm run release -- --prerelease beta",
    "serve:sw": "npm run build -s && npx http-server ./www -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.less\" --syntax less",
    "test": "ng test",
    "test:ci": "ng test --configuration=ci",
    "e2e:alpha": "ng e2e",
    "e2e:beta": "ng e2e --configuration=beta",
    "e2e": "ng e2e --prod",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "compodoc": "compodoc -p tsconfig.json -d docs/generated -s -o",
    "env": "ngx-scripts env npm_package_version FIREBASE_API_KEY USE_BACKEND_EMULATOR",
    "env:backend": "cd functions && firebase functions:config:get > .runtimeconfig.json",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,less}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,less}\"",
    "postinstall": "npm run env && webdriver-manager update && npm run install:backend",
    "cordova:prepare": "ngx-scripts cordova prepare",
    "cordova:run": "ngx-scripts cordova run",
    "cordova:build": "ngx-scripts cordova build --device --release --copy dist",
    "cordova:clean": "ngx-scripts clean",
    "cordova": "cordova",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^12.2.3",
    "@angular/common": "^12.2.3",
    "@angular/compiler": "^12.2.3",
    "@angular/core": "^12.2.3",
    "@angular/fire": "^7.0.2",
    "@angular/forms": "^12.2.3",
    "@angular/localize": "^12.2.3",
    "@angular/platform-browser": "^12.2.3",
    "@angular/platform-browser-dynamic": "^12.2.3",
    "@angular/router": "^12.2.3",
    "@angular/service-worker": "^12.2.3",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/keyboard": "^5.36.0",
    "@ionic-native/splash-screen": "^5.36.0",
    "@ionic-native/status-bar": "^5.36.0",
    "@ngrx/component": "^12.4.0",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/router-store": "^12.4.0",
    "@ngrx/schematics": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "@ngx-translate/core": "^13.0.0",
    "@stripe/stripe-js": "^1.17.1",
    "cordova-android": "^10.1.0",
    "cordova-custom-config": "^5.1.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.5",
    "firebase": "^9.0.1",
    "ng-zorro-antd": "^12.0.1",
    "ngx-stripe": "^12.4.0",
    "rxjs": "^6.5.3",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.1.1",
    "@angular-devkit/architect": "^0.1202.3",
    "@angular-devkit/build-angular": "^12.2.3",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "^12.2.3",
    "@angular/language-service": "^12.2.3",
    "@angularclass/hmr": "^3.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@compodoc/compodoc": "^1.1.14",
    "@ngx-rocket/scripts": "^5.1.1",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.8",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "cordova": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase-tools": "^9.16.6",
    "fuzzy": "^0.1.3",
    "hads": "^3.0.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^7.0.2",
    "inquirer": "^8.1.2",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^27.1.0",
    "nz-tslint-rules": "^0.901.2",
    "open": "^8.2.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "protractor": "~7.0.0",
    "puppeteer": "^10.2.0",
    "standard-version": "^9.3.1",
    "stylelint": "~13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "~22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-custom-config": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    }
  }
}
